<main class="main">
  <div class="inner">
    <% if user_signed_in? %>
    <div class="greeting">
      <%= "ようこそ" %>
      <%= link_to "#{current_user.nickname}さん", "/users/#{current_user.id}", class: "btn btn-link" %>
    </div> 
    <% end %>
    <div class="card__wrapper">
      <%= render partial: 'music', collection: @musics %>
    </div>
  </div>
</main>
<%= paginate(@musics) %>
